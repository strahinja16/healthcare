{
  "name": "healthcare-api",
  "version": "0.0.1",
  "engines": {
    "node": ">=10.0"
  },
  "description": "Healthcare api",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon ./src/bin/www",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo:all",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "db:reset": "npm run migrate:undo && npm run migrate",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:seed:undo": "node_modules/.bin/sequelize db:seed:undo:all",
    "db:build": "npm run db:reset && npm run db:seed",
    "db:create": "node_modules/.bin/sequelize db:create",
    "db:drop": "node_modules/.bin/sequelize db:drop"
  },
  "_moduleAliases": {
    "root": "./src",
    "config": "./src/config",
    "middleware": "./src/middleware",
    "models": "./src/models",
    "repositories": "./src/repositories",
    "requests": "./src/requests",
    "services": "./src/services",
    "resources": "./src/resources"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "http-status-codes": "^1.3.0",
    "joi": "^13.4.0",
    "joi-extension-semver": "^2.0.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "mailgun-js": "^0.20.0",
    "module-alias": "^2.1.0",
    "nodemailer": "^4.6.8",
    "nodemon": "^1.18.4",
    "moment": "^2.22.2",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0",
    "winston": "^3.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-import-resolver-alias": "^1.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "faker": "^4.1.0",
    "prettier": "^1.13.7",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  }
}
